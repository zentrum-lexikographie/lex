            <Verwendungsbeispiele>
                <Beleg class="invisible">
                    <Belegtext> Strom sparen. Kippschalter zum
                        <?oxy_comment_start author="jgrimm" timestamp="20250306T140622+0100" comment="Abwegiges Komma stillschw. entfernt" flag="done"?>Ausschalten<?oxy_comment_end?>
