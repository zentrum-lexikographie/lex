      <!-- D99 -->
      <value>noch</value>
      <!-- D99 -->
      <value>sonst</value>
      <!-- D99 -->
      <value>nicht</value>
    </choice>
  </define>
  <define name="Frequenzangabe">
    <element name="Frequenzangabe">
      <ref name="Frequenzangaben"/>
    </element>
  </define>
  <define name="Frequenzbeschraenkung">
    <attribute name="Frequenz">
      <ref name="Frequenzangaben"/>
    </attribute>
  </define>
