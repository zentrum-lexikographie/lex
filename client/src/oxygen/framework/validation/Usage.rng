  <define name="Frequenzangaben">
    <a:documentation/>
    <!-- Markierungen ############## -->
    <a:documentation/>
    <choice>
      <value>besonders</value>
      <!-- D99 -->
      <value>häufig</value>
      <value>meist</value>
      <value>oft</value>
      <!-- D99, selten auch: öfter -->
