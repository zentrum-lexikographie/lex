<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" datatypeLibrary="">
  <define name="Diachronie">
    <a:documentation/>
    <!--
      Etymologie
      (nicht konsistent modelliert,
      da in der lexikografischen Bearbeitung vorerst zurückgestellt)
    -->
    <a:documentation/>
    <element name="Diachronie">
      <group>
        <optional>
          <ref name="Metadaten.allgemein"/>
        </optional>
        <optional>
          <attribute name="class">
            <value>invisible</value>
          </attribute>
        </optional>
        <!-- TODO: remove @class here -->
        <element name="Etymologie">
          <optional>
            <attribute name="class">
              <value>invisible</value>
            </attribute>
          </optional>
          <ref name="freie_Beschreibung"/>
        </element>
        <element name="Formgeschichte">
          <optional>
            <attribute name="class">
              <value>invisible</value>
            </attribute>
          </optional>
          <ref name="freie_Beschreibung"/>
        </element>
        <element name="Bedeutungsgeschichte">
          <optional>
            <attribute name="class">
              <value>invisible</value>
            </attribute>
          </optional>
          <ref name="freie_Beschreibung"/>
        </element>
        <optional>
          <element name="Datierung">
            <zeroOrMore>
              <element>
                <anyName/>
                <data type="token"/>
              </element>
            </zeroOrMore>
          </element>
        </optional>
      </group>
      <!-- TODO: remove? -->
    </element>
  </define>
  <define name="freie_Beschreibung">
    <mixed>
      <interleave>
        <zeroOrMore>
          <ref name="Paraphrase"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="erwaehntes_Zeichen"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Stilebene"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Stilfaerbung"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Sprachraum"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Gebrauchszeitraum"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Gruppensprache"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Fachgebiet"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="semantischer_Verweis"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="externer_Verweis"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="Fundstelle"/>
        </zeroOrMore>
        <zeroOrMore>
          <element name="Stichwort">
            <ref name="Lemmatoken"/>
          </element>
        </zeroOrMore>
      </interleave>
    </mixed>
  </define>
  <define name="Kommentar">
    <element name="Kommentar">
      <optional>
        <attribute name="class">
          <value>invisible</value>
        </attribute>
      </optional>
      <ref name="freie_Beschreibung"/>
    </element>
  </define>
  <define name="erwaehntes_Zeichen">
    <element name="erwaehntes_Zeichen">
      <attribute name="Sprache">
        <list>
          <oneOrMore>
            <ref name="Sprachen"/>
          </oneOrMore>
        </list>
      </attribute>
      <data type="token"/>
    </element>
  </define>
