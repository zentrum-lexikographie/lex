#!/usr/bin/env python

from os import execlp

execlp(
    'docker',
    'docker',
    'run',
    '-t', '-d',
    '--name', 'zdl_lex_mysql',
    '-e', 'MYSQL_DATABASE=dwdswb',
    '-e', 'MYSQL_USER=dwdswb',
    '-e', 'MYSQL_PASSWORD=dwdswb',
    '-e', 'MYSQL_RANDOM_ROOT_PASSWORD=yes',
    '-p', '3306:3306',
    'mysql:5',
    '--character-set-server=utf8mb4',
    '--collation-server=utf8mb4_unicode_ci'
)
